<%if false%>
  <div class="">
    <a href="<%= current_user ? user_path(user.address) : '#'%>" class="<%=current_user ? '' : 'show-login-message'%>">
      <div class="featured-creators-box">
        <div class="featured-creators-box-img">
          <div class="featured-creators-box-img-bg" style="background-image: url(<%= url_for(user.banner_image) %>);">
            <%= image_tag url_for(user.banner_image(1300)), class: 'img-responsive' %>
          </div>
          <div class="featured-creators-box-img-profile">
            <%= image_tag url_for(user.profile_image(:thumb)), class: 'img-responsive' %>
          </div>
        </div>
        <div class="featured-creators-box-content">
          <h4><%= user.full_name %></h4>
        </div>
      </div>
    </a>
  </div>
<%end%>
<div class="feat-user-box">
  <a href="<%= user_path(user.address)%>" class="feat-user-a">
    <div class="feat-user-img">
      <%= image_tag url_for(user.banner_image(1300)), class: 'img-responsive' %>
    </div>
    <div class="feat-user-con">
      <div class="feat-user-pro">
        <%= image_tag url_for(user.profile_image(:thumb)), class: 'img-responsive' %>
      </div>
      <div class="feat-user-hd">
        <h4><%= user.full_name %></h4>
      </div>
    </div>
  </a>
</div>
